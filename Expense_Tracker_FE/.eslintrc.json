{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": "airbnb",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"indent": ["error", 4],
		"react/jsx-indent": ["error", 4],
		"react/jsx-indent-props": ["error", 4],
		"no-underscore-dangle": 0,
		"react/forbid-prop-types": 0
	}
}
